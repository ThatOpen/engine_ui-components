    "ribbon",
    "toolbar"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ThatOpen/engine_ui-components#readme",
  "bugs": {
    "url": "https://github.com/ThatOpen/engine_ui-components/issues"
